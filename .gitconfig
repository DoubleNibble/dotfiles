[core]
        excludesfile = ~/.gitignore_global
        visual = 'emacs' --no-splash -q -nw
        editor = 'emacs' --no-splash -q -nw
[user]
	name = myTerminal
	email = team.terminal@gmail.com
[alias]
        st = status
        lg = log
        ps = push origin
        pl = pull origin
        cm = commit
        co = checkout
        df = diff
