[core]
        excludesfile = ~/.gitignore_global
        visual = 'emacs' --no-splash -q -nw
        editor = 'emacs' --no-splash -q -nw
[user]
	name = myTerminal
	email = team.terminal@gmail.com
[alias]
        st = "! clear && git status"
        df = "! clear && git diff"
        lg = log
        co = checkout
        rs = reset
        fc = fetch
        cm = commit
        pl = pull origin
        down = pull origin master
        ps = push origin
        up = push origin master
        sync = "! sh ~/.scripts/common/update-all-projects.sh"
        sync-all = "!git sync ~/_repositories/"
